    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dry Run (local mbox)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "mbox-to-imap",
                "--dry-run",
                "--mbox",
                "test_data/All mail Including Spam and Trash.mbox",
                "--imap-host",
                "127.0.0.1",
                "--imap-user",
                "test",
                "--imap-pass",
                "qwert",
                "--target-folder",
                "DEBUG",
                "--log-level",
                "debug",
                "--log-dir",
                ".",
                "--state-dir",
                "."
            ]
        },
        {
            "name": "mbox-stats",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "mbox-stats",
                "test_data/All mail Including Spam and Trash.mbox"
            ]
        }
    ]
}